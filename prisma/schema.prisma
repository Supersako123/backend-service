// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Dog {
  id          String   @id
  name        String
  description String
  lifeMin Int
  lifeMax Int
  weightMaleMin   Int
  weightMaleMax   Int
  weightFemaleMax   Int
  weightFemaleMin   Int
  hypoallergenic Boolean
}

